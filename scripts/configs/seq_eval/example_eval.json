        "cut_last_frame": false,
        "val_posDistThr": 2,
        "seq_gt_strategy": "lax",
        "reverse": false
      }
    },
    "oxford1_v": {
      "data": {
        "dataset_path": "/home/user/datasets/oxford_robotcar_v",
        "split": "1",
        "seq_length": 5,
