{
  "experiment": {
    "name": "casevpr_hvpr_eval",
    "logs_root": "output/seq_eval_logs",
    "seed": 43,
    "deterministic": true,
    "infer_batch_size": 1,
    "save_preds_json": false,
    "save_gif": false,
    "save_html": false
  },
  "model_variants": {
    "CaseNet": {
      "model": {
        "seq_encoder": "hvpr_casevpr_224_crica"
      }
    },
    "CaseNet_ft": {
      "model": {
        "seq_encoder": "hvpr_casevpr_224"
      }
    },
    "SeqVLAD": {
      "model": {
        "seq_encoder": "vgg16_seqvlad"
      }
    },
    "JIST": {
      "model": {
        "seq_encoder": "jist"
      }
    },
    "SVPR": {
      "model": {
        "seq_encoder": "svpr"
      }
    },
    "SeqNet": {
      "model": {
        "seq_encoder": "hvpr_seqnet"
      }
    }
  },
  "dataset_variants": {
    "oxford1": {
      "data": {
        "dataset_path": "/home/user/datasets/oxford_robotcar",
        "split": "1",
